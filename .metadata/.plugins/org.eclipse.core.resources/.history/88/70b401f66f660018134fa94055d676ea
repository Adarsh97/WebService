package com.ms.pckg1;

public class Person {

	private String name;
	private int age;
	private int id;
	private int salary;
	
	public Person(int id,String name,int age,int salary)
	{
		this.id=id;
		this.age=age;
		this.name=name;
		this.salary=salary;
	}
	
	public int  getId()
	{
		return id;
	}
	public int getSalary()
	{
		return salary;
	}
	public int getAge()
	{
		return age;
	}
	public String getName()
	{
		return name;
	}
	public void setAge(int age)
	{
		this.age=age;
	}
	public void setId(int id)
	{
		this.id=id;
	}
	public void setName(String name)
	{
		this.name=name;
	}
	public void setSalary(int salary)
	{
		this.salary=salary;
	}
	
}
